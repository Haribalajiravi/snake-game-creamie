    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-loader": "^8.0.6",
    "node-watch": "^0.6.3",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
